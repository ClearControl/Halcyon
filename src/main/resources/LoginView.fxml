<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="demo.mvc.LoginView">
    <children>
        <VBox alignment="CENTER_RIGHT" spacing="10.0">
            <children>
                <TextField fx:id="userNameTextField" alignment="BOTTOM_RIGHT" promptText="Username" />
                <PasswordField fx:id="passwordTextField" alignment="CENTER_RIGHT" promptText="Password" />
                <Button fx:id="loginButton" mnemonicParsing="false" text="Login" textAlignment="CENTER" />
                <TextField fx:id="sliderValueTextField" alignment="BOTTOM_RIGHT" promptText="Username" />
                <Slider fx:id="slider" layoutX="157.0" layoutY="17.0" orientation="VERTICAL" />
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </VBox>
    </children>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
</AnchorPane>